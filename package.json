  "license": "BSD-3-Clause",
  "repository": "https://github.com/unicef-polymer/etools-frontend-template",
  "scripts": {
    "tsToJs": "tsc --skipLibCheck",
    "esLint": "./node_modules/eslint/bin/eslint.js 'src_ts/**'",
    "lint": "npm run esLint",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "start": "gulp serve",
    "build": "tsc  --skipLibCheck && export NODE_OPTIONS=--max_old_space_size=4096 && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
