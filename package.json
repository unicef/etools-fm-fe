    "typescript": "^4.5.4",
    "ua-parser-js": ">0.7.31",
    "ajv": ">6.12.6",
    "follow-redirects": ">=1.14.9",
    "shelljs": ">=0.8.5",
    "node-fetch": ">=2.6.7",
