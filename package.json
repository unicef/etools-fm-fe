    "del": "^3.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-lit": "^1.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "polymer-cli": "^1.9.11",
    "prettier": "^2.2.1",
    "typescript": "^3.9.9"
  }
}
