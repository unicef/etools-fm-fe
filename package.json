    "@types/ramda": "^0.26.43",
    "@unicef-polymer/etools-ajax": "^4.3.3",
    "@unicef-polymer/etools-app-selector": "^3.1.8",
    "@unicef-polymer/etools-behaviors": "^3.0.4",
    "@unicef-polymer/etools-content-panel": "5.0.0-rc.2",
