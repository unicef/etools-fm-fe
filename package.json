{
  "name": "etools-fm",
  "description": "Etools Field Monitoring",
  "version": "2.0.0",
  "contributors": [
    "eTools Team"
  ],
  "license": "BSD-3-Clause",
  "repository": "https://github.com/unicef-polymer/etools-frontend-template",
  "scripts": {
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && rollup -c rollup-prod.config.js --configPlugin @rollup/plugin-commonjs",
    "tsToJs": "tsc --skipLibCheck || true",
    "start": "rm -rf src && npm install && rollup -c rollup-dev.config.js --configPlugin @rollup/plugin-commonjs -w",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "generate:icons": "npm --prefix node_modules/@unicef-polymer/etools-unicef run generate-icons -- --output=$(pwd)/assets/icons/ --icons=$(pwd)/assets/icons/icons.js",
    "rd:init": "relative-deps init",
    "rd:add": "relative-deps add",
    "rd:reload": "relative-deps",
    "rd:watch": "chokidar \"../../etools-polymer/**/*.js\" \"../../etools-polymer/**/**/*.js\" -c 'npm run rd:reload' --silent",
    "prepare": "relative-deps"
  },
  "dependencies": {
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "@shoelace-style/shoelace": "2.18.0",
    "@types/leaflet": "^1.9.3",
    "@types/lodash-es": "^4.17.7",
    "@types/ramda": "0.30.2",
    "@unicef-polymer/etools-form-builder": "^3.1.0",
    "@unicef-polymer/etools-modules-common": "^5.1.2",
    "@unicef-polymer/etools-piwik-analytics": "^4.1.0",
    "@unicef-polymer/etools-types": "^1.11.17",
    "@unicef-polymer/etools-unicef": "^1.1.1",
    "@unicef-polymer/etools-utils": "^2.0.1",
    "dayjs": "^1.11.10",
    "dexie": "^3.2.4",
    "esri-leaflet": "^3.0.8",
    "leaflet": "^1.9.2",
    "leaflet.markercluster": "^1.5.3",
    "lit": "^3.2.1",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
    "ramda": "0.30.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "relative-deps": "^1.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@typescript-eslint/typescript-estree": "^5.7.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "~2.5.1",
    "rollup": "^4.28.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-license": "^3.5.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^3.0.0",
    "rollup-plugin-workbox": "^8.1.1",
    "socket.io": ">=4.6.1",
    "socket.io-parser": ">=4.2.3",
    "svg-sprite": "^2.0.2",
    "typescript": "^4.9.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-musl": "^4.28.0"
  },
  "relativeDependencies": {}
}
