    "@webcomponents/webcomponentsjs": "^2.5.0",
    "dayjs": "^1.10.2",
    "dexie": "^3.0.3",
    "etools-piwik-analytics": "git+https://github.com/unicef-polymer/etools-piwik-analytics.git#upgrade-to-polymer3",
    "leaflet": "^1.7.1",
    "lit-element": "^2.4.0",
    "lit-translate": "^1.2.1",
    "pwa-helpers": "^0.9.1",
    "ramda": "^0.27.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
