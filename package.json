{
  "name": "etools-apps-base-shell",
  "description": "Etools Apps Base - starting point for etools apps - based on PWA starter kit",
  "version": "2.0.0",
  "contributors": [
    "eTools Team"
  ],
  "license": "BSD-3-Clause",
  "repository": "https://github.com/unicef-polymer/etools-frontend-template",
  "scripts": {
    "tsToJs": "tsc --skipLibCheck",
    "esLint": "./node_modules/eslint/bin/eslint.js 'src_ts/**'",
    "lint": "npm run esLint",
    "start": "gulp serve",
    "build": "tsc  --skipLibCheck && npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.1.0",
    "@polymer/app-localize-behavior": "^3.0.0-pre.20",
    "@polymer/iron-autogrow-textarea": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.2",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-radio-button": "^3.0.1",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-tabs": "^3.1.0",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.3.0",
    "@types/leaflet": "^1.5.1",
    "@types/ramda": "^0.26.19",
    "@unicef-polymer/etools-ajax": "^4.0.0",
    "@unicef-polymer/etools-app-selector": "^3.1.1",
    "@unicef-polymer/etools-data-table": "^3.0.0-rc.1",
    "@unicef-polymer/etools-date-time": "^1.0.0-rc.3",
    "@unicef-polymer/etools-dialog": "^3.0.0",
    "@unicef-polymer/etools-dropdown": "^3.0.6",
    "@unicef-polymer/etools-loading": "^4.0.1",
    "@unicef-polymer/etools-profile-dropdown": "4.0.1-rc.1",
    "@unicef-polymer/etools-upload": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "dexie": "^2.0.4",
    "leaflet": "^1.5.1",
    "lit-element": "^2.2.1",
    "lit-translate": "^1.1.20",
    "moment": "^2.24.0",
    "pwa-helpers": "^0.9.1",
    "ramda": "^0.26.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@polymer/decorators": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "del": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "polymer-cli": "^1.9.2",
    "polyserve": "^0.27.0",
    "prettier": "^1.19.1",
    "prpl-server": "^1.3.0",
    "typescript": "^3.6.4"
  }
}
