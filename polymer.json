{
  "entrypoint": "index.html",
  "shell": "src/components/app-shell/app-shell.js",
  "sources": [
    "images/**/*"
  ],
  "fragments": [
    "src/components/pages/page-not-found.js",
    "src/components/pages/settings/settings-page.js",
    "src/components/pages/settings/sites-tab/sites-tab.js",
    "src/components/pages/settings/questions-tab/questions-tab.js",

    "src/components/pages/plan/plan-page.js",
    "src/components/pages/plan/templates-tab/templates-tab.js",
    "src/components/pages/plan/rationale-tab/rationale-tab.js",

    "src/components/pages/activities-and-data-collection/activities-page.js",

    "src/components/pages/analyze/analyze-page.js"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/web-animations-js/web-animations-next-lite.min.js",
    "node_modules/leaflet/dist/leaflet.js",
    "node_modules/moment/min/moment.min.js",
    "push-manifest.json"
  ],
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
