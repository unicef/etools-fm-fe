{
  "entrypoint": "index.html",
  "shell": "src/components/app-shell/app-shell.js",
  "sources": [
    "images/**/*"
  ],
  "fragments": [
    "src/components/pages/page-not-found.js",
    "src/components/pages/settings/settings-page.js",
    "src/components/pages/settings/sites-tab/sites-tab.js",
    "src/components/pages/settings/questions-tab/questions-tab.js",

    "src/components/pages/page-two.js",
    "src/components/pages/engagements/engagements-list.js",
    "src/components/pages/engagements/engagement-tabs.js",
    "src/components/pages/engagements/engagement-tab-pages/engagement-details.js",
    "src/components/pages/engagements/engagement-tab-pages/engagement-questionnaires.js"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "push-manifest.json"
  ],
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
